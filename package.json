{
  "name": "omnomcom-application",
  "version": "1.0.1",
  "description": "Electron powered OmNomCom application",
  "repository": {
    "type": "git",
    "url": "https://github.com/saproto/OmNomCom-App/"
  },
  "main": "main.js",
  "author": "Jesse Visser",
  "private": true,
  "scripts:": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "build": "electron-builder --mac --windows --linux",
    "release": "electron-builder --mac --windows --linux --publish always"
  },
  "build": {
    "appId": "saproto.omnomcom",
    "productName": "OmNomCom Application"
  },
  "dependencies": {
    "update-electron-app": "^2.0.1"
  },
  "devDependencies": {
    "electron": "^13.1.4",
    "electron-builder": "^22.11.7"
  }
}
