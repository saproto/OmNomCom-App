{
  "name": "omnomcom-application",
  "version": "2.0.0",
  "description": "Electron powered OmNomCom application",
  "main": "main.js",
  "author": "Jesse Visser",
  "private": true,
  "scripts:": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
